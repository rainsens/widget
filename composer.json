{
    "name": "rainsens/widget",
    "description": "A group of widget based on rainsens/asset",
    "authors": [
        {
            "name": "Rainsen",
            "email": "yusen@rainsen.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "laravel/framework": "^6.2",
        "rainsens/asset": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Rainsens\\Widget\\": "src"
        },
        "files": [
            "bootstrap/helpers.php",
            "bootstrap/buttons.php",
            "bootstrap/dropdowns.php",
            "bootstrap/nestables.php",
            "bootstrap/forms.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rainsens\\Widget\\Providers\\WidgetServiceProvider"
            ]
        }
    }
}
